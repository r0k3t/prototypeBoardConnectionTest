    								<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Panel.Background).(RadialGradientBrush.RadiusX)" Storyboard.TargetName="Highlight">
    									<EasingDoubleKeyFrame KeyTime="0" Value="0.759"/>
